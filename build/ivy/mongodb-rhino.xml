<?xml version="1.0" encoding="UTF-8"?>
<ivy-module version="2.0">
	<info
		organisation="${maven.group}"
		module="${maven.artifact}"
		revision="${version}"
		status="release"
		default="true" />
	<configurations>
		<conf name="default" visibility="public" />
	</configurations>
	<dependencies>
		<dependency org="org.mozilla" name="rhino" rev="1.7R3" transitive="false" />
		<dependency org="com.threecrickets.rhino" name="json-rhino" rev="1.0" transitive="false" />
		<dependency org="com.mongodb" name="java-driver" rev="2.7.3" transitive="false" />
	</dependencies>
</ivy-module>
